/* pages/project/data-detail/index.wxss */
page{
  background-color: rgb(245,245,245)
}
.top{
  background-color: #fff;
   padding: 0 23rpx;
}
.top__head{
  font-size: 36rpx;
  color: rgb(1,1,1)
}
.top__developer{
  font-size: 24rpx;
  color: rgb(193,193,193);
  padding:15rpx 0 
}
.imgList{
  background: #fff;
  padding: 30rpx 23rpx;
  line-height: .7 
}
.imgList__img{
  display: inline-block;
  position: relative;
}
.imgList__img image{
  width: 180rpx;
  height: 180rpx;
}
.more{
  position: absolute;
  width: 180rpx;
  height: 180rpx;
  background-color: #000;
  opacity: 0.5;
  top: 0;
  font-size: 80rpx;
  text-align: center;
  color: red;
  line-height: 180rpx;
}
.footer {
  position: absolute;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 100rpx;
  font-size: 24rpx;
  color: #000;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-around
}
.footer .iconfont{
  margin-right: 10rpx
}