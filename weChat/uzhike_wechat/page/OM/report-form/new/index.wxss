page{
  font-size: 30rpx;
}
.list{
  height: 100rpx;
  padding: 0 25rpx;
  background-color: #fff;
  display: flex;
  align-items: center;
}

.around{
  justify-content: space-between
}
.name{
  margin-left: 30rpx
}
.list input{
  width: 500rpx
}
.list view{
  display: flex;
  align-items: center;
}
.add{
  height: 100rpx;
  width: 100%;
  line-height: 100rpx;
  background-color: #fff;
  color:rgb(255,103,20);
  border-top: 1rpx solid #f5f5f5
}
.warn{
  height: 55rpx;
  line-height: 55rpx;
  padding: 0 25rpx;
  font-size: 24rpx;
  color: rgb(193, 193, 193)
}
.shanchu{
  color: rgb(219,219,219);
  margin-right: 20rpx;
}
.tianjia{
  margin-right: 20rpx;
}
.arrow_left{
  color: rgb(210,210,210);
  margin-left: 20rpx
}

.footer {
  position: fixed;
  left: 0;
  bottom: 0;
  padding: 10rpx 36rpx;
  width: 100%;
  background-color: #fff;
}